UAG(DEFAULT) {
    "client"
}

AUTHORITY("EPICS Root Certificate Authority") {
    AUTHORITY(CMS_AUTH, "intermediateCA")
}

UAG(BAR) {
    "michael"
}

ASG(RO) {
    RULE(1,READ,TRAPWRITE)
}

ASG(SPECIAL) {
    RULE(1,WRITE,TRAPWRITE) {
        UAG(DEFAULT)
        UAG(BAR)
	AUTHORITY(CMS_AUTH)
	METHOD(x509)
	PROTOCOL(tls)
    }
}
